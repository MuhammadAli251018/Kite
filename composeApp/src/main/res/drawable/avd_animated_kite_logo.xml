                </group>
                <group android:name="group_bl">
                    <path
                        android:name="bottom_left"
                        android:pathData="M 82.82 199.23 C 82.28 200.84 84.27 202.07 85.47 200.87 L 160.72 125.62 C 161.38 124.96 161.34 123.88 160.63 123.27 L 121.56 89.97 C 120.69 89.24 119.36 89.61 119 90.69 L 82.82 199.23 Z"
                        android:fillColor="#ffffff"/>
                </group>
            </group>
        </vector>
    </aapt:attr>
    <target android:name="group_tr">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:propertyName="translateX"
                android:startOffset="200"
                android:duration="800"
                android:valueFrom="30"
                android:valueTo="0"
                android:valueType="floatType"
                android:interpolator="@android:anim/anticipate_interpolator"/>
        </aapt:attr>
    </target>
    <target android:name="group_tl">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:propertyName="translateY"
                android:startOffset="200"
                android:duration="800"
                android:valueFrom="-30"
                android:valueTo="0"
                android:valueType="floatType"
                android:interpolator="@android:anim/anticipate_interpolator"/>
        </aapt:attr>
    </target>
    <target android:name="group_br">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:propertyName="translateY"
                android:startOffset="200"
                android:duration="800"
                android:valueFrom="30"
                android:valueTo="0"
                android:valueType="floatType"
                android:interpolator="@android:anim/anticipate_interpolator"/>
        </aapt:attr>
    </target>
    <target android:name="group_bl">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:propertyName="translateX"
                android:startOffset="200"
                android:duration="800"
                android:valueFrom="-30"
                android:valueTo="0"
                android:valueType="floatType"
                android:interpolator="@android:anim/anticipate_interpolator"/>
        </aapt:attr>
    </target>
    <target android:name="group">
        <aapt:attr name="android:animation">
            <set>
                <objectAnimator
                    android:propertyName="scaleX"
                    android:startOffset="250"
                    android:duration="750"
                    android:valueFrom="0"
                    android:valueTo="1"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/decelerate_interpolator"/>
                <objectAnimator
                    android:propertyName="scaleY"
                    android:startOffset="250"
                    android:duration="750"
                    android:valueFrom="0"
                    android:valueTo="1"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/decelerate_interpolator"/>
            </set>
        </aapt:attr>
    </target>
    <target android:name="top_right">
        <aapt:attr name="android:animation">
            <set>
                <objectAnimator
                    android:propertyName="fillColor"
                    android:duration="250"
                    android:valueFrom="#00ffffff"
                    android:valueTo="#00ffffff"
                    android:valueType="colorType"
                    android:interpolator="@android:anim/linear_interpolator"/>
                <objectAnimator
                    android:propertyName="fillColor"
                    android:startOffset="300"
                    android:duration="600"
                    android:valueFrom="#00ffffff"
                    android:valueTo="#ffffff"
                    android:valueType="colorType"
